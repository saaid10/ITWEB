script.
    function goToDetails(id) {
        window.location.href = '/workout/'+id+ '/add'
    }

div(class="workoutcard")
    h2= workoutProgram.name
    hr
    each excersise in workoutProgram.workouts
        p 
            b #{excersise.exercise}: (#{excersise.repsOrTime} / #{excersise.set})
    
    div(class="buttons")
        button(onClick="goToDetails(" + workoutProgram._id + ")") Show Details
        //- button(onclick='authenticate(\'' + url + '\',\'' + token + '\',\'' + accountId + '\')') Login
        button(onClick) Add workout to program

